display: flex;
flex-flow: row;
padding:0 40rpx;
height: 212rpx
}
.signImg{
width: 133rpx;
text-align: center;
padding:40rpx 0
}
.signImg image{
width: 133rpx;
height:133rpx;
border-radius:16rpx
}
.signCenter{
width:80%;
padding:35rpx 20rpx
}
.signCenter .toptitle{
  font-size:28rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.signDate{
  font-size: 24rpx;
