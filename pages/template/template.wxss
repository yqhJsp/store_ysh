}
.num{
  vertical-align:5rpx
}
.signBtn,.canBtn{
  width:108rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size:24rpx;
  position:absolute;
  bottom:25rpx;
  /* background:#2299ff; */
