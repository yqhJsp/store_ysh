/* pages/vote/oneDetail/oneDetail.wxss */
page{
  background:#f5f5f5
}
.row{
   box-sizing: border-box;
   font-size: 26rpx;  
}
.coverImg image{
  width: 100%;
  height:500rpx
}
.consulTitle{
  padding:30rpx;
  background:#fff;
}
.detailTitle {
  padding:20rpx 30rpx;
  margin: 20rpx 0 0 0;
  position: relative;
  background:#fff
}
.fristTop {
  display: flex;
  flex-flow: row;
}
.voteTitle {
  font-size:30rpx;
  color: #333;
  width: 80%;
  overflow: hidden;
  display: -webkit-box;
  text-overflow: ellipsis;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  padding-top: 10rpx
}
.voteShare {
  text-align: right;
  display: block;
  width: 20%;
}
.shareBtn {
  width: 110rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
  color: #fff;
  display: inline-block;
  background: #29f;
  border-radius: 8rpx;
  text-align: center;
  margin-top: 10rpx;
}
button::after {
  width: 0;
  height: 0;
}
/*视频*/
.videoBox {
  width: 100%;
  display: inline-block;
  background-color: #fff;
}

.videoBox .boxImg {
  width: 100%;
  overflow: hidden;
  text-align:center
}

.videoBox.boxImg video {
  width: 100%;
  height: 100%;
}

.videoBox .boxcoent {
  text-align: center;
  font-size: 26rpx;
  color: #333;
  padding: 20rpx;
}

.videoBox .boxcoent view {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.subhead {
  color: #999;
}

.cover, .coverImg {
  position: relative;
  width: 100%;
}

.playImg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 350rpx;
}



