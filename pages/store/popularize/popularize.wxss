/* pages/marketing/deposit/deposit.wxss */

.depContaint {
  margin-top: 30rpx;
  padding: 30rpx 30rpx;
  background: #fff;
}

.deptitle {
  font-size: 28rpx;
}

.depceter {
  height: 90rpx;
  border: 1rpx solid #ccc;
  display: flex;
  flex-flow: row;
  margin-top: 20rpx;
}

.depLeft {
  width: 70%;
  display: flex;
  flex-flow: row;
}

.depRigth {
  width: 35%;
  text-align: left;
  color: #409fff;
  font-size: 24rpx;
  margin-top:30rpx;
}

.depInput {
  height: 90rpx;
  font-size: 32rpx;
  padding: 0 15rpx;
  border: none;
  width: 90%;
}
.desWay{
 display:flex;
 flex-flow: row;
 font-size: 28rpx;
 margin-top: 100rpx
}
.userRight {
  width:70%;
  display: flex;
  flex-flow: row;
}
.nameText{
  width:25%
}
.parcelItem {
  width: 40%;
  padding-top:5rpx
}

.parcelItem label {
  padding-left: 20rpx;
  font-size: 28rpx;
  vertical-align:5rpx;
  display: inline-block;
}
.depIcon {
  width: 10%;
  padding-top: 50rpx;
}

.tipcolor {
  color: #f44336;
}
.checkBox{
  margin-top:0
}
.pretable {
  font-size: 28rpx;
  margin-top:50rpx
}
.distr {
  margin-top: 20rpx;
}

.distr .distd {
  padding: 0 20rpx;
  height: 90rpx;
  background-color: #fff;
  line-height: 90rpx;
  display: flex;
  flex-flow: row;
}

.boder {
  border-bottom: #dedede 1rpx solid;
}

.prerow {
  width:25%;
}

.prerow2 {
  width: 80%;
  text-align: left;
}
.integInput {
  border: none;
  display: inline-block;
  line-height: 90rpx;
  height: 90rpx;
  padding-left: 20rpx;
  width: 100%;
}
.mod_btn {
  background: #f44336;
  color: #fff;
  display: block;
  border: 0;
  margin: 0 auto;
  width: 90%;
  height: 92rpx;
  margin-top:150rpx;
  line-height: 92rpx;
  text-align: center;
  font-size: 32rpx;
  border-radius: 16rpx;
  position: relative;
}

.deps {
  color: #999;
  font-size: 24rpx;
  margin-top: 10rpx;
}
/*弹窗*/
.modalPop{
  width: 85%
}
.popContent{
 width:100%;
 height:200rpx;
 margin-top:20rpx;
 text-align:center
}
.pimages{
 width:200rpx;
 height:200rpx   
}
.voteTip{
  text-align: center;
  font-size:28rpx;
  padding:40rpx 20rpx

}
.report{
 padding:0 40rpx;
}
