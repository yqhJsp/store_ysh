/* pages/mall/templates/templates.wxss */

.detail-nav {
  display: flex;
  flex-direction: row;
  align-items: center;
  float: left;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  right: 0;
  z-index: 100;
  width: 100%;
  border-top: 1px solid #ddd;
  height: 100rpx;
  overflow: hidden;
}

.commetNav {
  width: 50%;
}

.view-left-search {
  height: 54rpx;
  text-align: left;
  border-radius: 50rpx;
  background: #f4f5f7;
  overflow: hidden;
  width: 50%;
  margin-left: 20rpx;
  display: flex;
  flex-flow: row;
}

.view-left-search .search {
  height: 54rpx;
  width: 80%;
  border: 0;
  color: #333;
  font-size: 28rpx;
  padding: 0 15rpx;
  line-height: 54rpx;
}

.view-left-search .mall-icon-search {
  margin: 0 20rpx;
}

.view-left-search .mall-icon-search  image {
  width: 37rpx;
  height: 34rpx;
}

.commentab {
  width: 20%;
  text-align: center;
}

.padding {
  padding: 0;
}

.dIcon2 {
  width: 34rpx;
  height: 34rpx;
}

.dIcon1 {
  width: 39rpx;
}

.dIcon3 {
  width: 36rpx;
}

.line_nav {
  width: 5rpx;
  height: 100%;
  background-color: gainsboro;
}

.descont {
  padding: 5rpx 0 0 5rpx;
}

.onlineBtn {
  border: none;
  background-color: #fff;
  font-size: 24rpx;
  padding: 10rpx 30rpx;
  display: flex;
  flex-flow: row;
  margin-top: 15rpx;
  text-align: center;
}

button[class="onlineBtn"]::after {
  border: 0;
}

.collect {
  color: #29f;
}

/*列表*/

.signOne {
  margin-top: 20rpx;
  background: #fff;
  border-radius: 16rpx;
  position: relative;
  box-shadow: #dedede 0px 0px 8px;
  display: flex;
  flex-flow: row;
  padding: 0 40rpx;
  height: 212rpx;
}

.signImg {
  width: 133rpx;
  text-align: center;
  padding: 40rpx 0;
}

.signImg image {
  width: 133rpx;
  height: 133rpx;
  border-radius: 16rpx;
}

.signCenter {
  width: 80%;
  padding: 35rpx 20rpx;
}

.signCenter .toptitle {
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.signDate {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}

.signNum {
  color: #29f;
  font-size: 24rpx;
  margin-top: 20rpx;
}

.signicon {
  height: 28rpx;
  width: 26rpx;
  display: inline-block;
  margin-right: 10rpx;
}

.num {
  vertical-align: 5rpx;
}

.signBtn {
  width: 108rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  bottom: 25rpx;
  right: 40rpx;
  background: #29f;
  border-radius: 8rpx;
  text-align: center;
}

.signActive {
  background: #ccc;
}

.tab-content1 {
  height: 100%;
  align-items: center;
  vertical-align: middle;
  text-align: center;
}

.tab-image {
  margin-top: 130rpx;
  margin-bottom: 50rpx;
  width: 252rpx;
}

/*优惠卷*/

.couponOne {
  width: 500rpx;
  height: 206rpx;
  border: 1rpx solid #efefef;
  box-shadow: 0px 5rpx 15rpx #efefef;
  flex: 1;
  margin: 0 24rpx 0 0;
  border-radius: 8rpx;
  display: flex;
  flex-flow: row;
  background: #fff;
  position: relative;
}

.mtright {
  margin-right: 24rpx;
}

.couLeft {
  width: 100%;
  padding-left: 30rpx;
}

/* .couLeft {
  width: 70%;
  padding-left: 30rpx;
} */

/* .couRigth {
  width: 30%;
  text-align: center;
} */

.couRigth {
  width: 104rpx;
  text-align: center;
  position: absolute;
  top: 0rpx;
  right: 20rpx;
}

.couprice {
  font-size: 90rpx;
  vertical-align: text-top;
}

.coutip {
  font-size: 30rpx;
  vertical-align: -10rpx;
}

.couRed {
  color: #45cdc6;
}

.coudate {
  font-size: 18rpx;
  color: #999;
}

.coubttom {
  font-size: 24rpx;
  color: #333;
  margin-top: 10rpx;
}

.getcoup, .delcoup {
  height: 104rpx;
  width: 104rpx;
  border-radius: 100%;
  font-size: 28rpx;
  text-align: center;
  margin-top: 20rpx;
  display: inline-block;
}

.getcoup {
  background: #45cdc6;
  color: white;
}

.coufont {
  width: 60rpx;
  display: inline-block;
  margin-top: 15rpx;
}

.delcoup {
  background: #f5f5f5;
  color: #333;
  line-height: 104rpx;
}

/*门店*/
.storeOne {
  display: flex;
  flex-flow: row;
  padding: 30rpx 24rpx;
  border-top: 1rpx solid #f0f0f0;
}

.storeLeft {
  width: 126rpx;
}

.storeLeft image {
  width: 126rpx;
  height: 126rpx;
  border-radius:100%;
  box-shadow: #999 0 0 10rpx;
}

.storeCenter {
  width: 60%;
  padding: 0 20rpx;
}

.centerTitle {
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.centerIcon {
  margin-top: 10rpx;
}

.storeCon {
  height: 40rpx;
  padding: 0 25rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #666;
  border-radius: 8rpx;
  margin-right: 15rpx;
  background: #eee;
  text-align: center;
  display: inline-block;
}

.centerAddress {
  margin-top: 40rpx;
  display: flex;
  flex-flow: row;
}

.adicon {
  width: 22rpx;
  height: 25rpx;
}

.address {
  font-size: 24rpx;
  color: #999;
  margin-left: 10rpx;
  display: inline-block;
  vertical-align: 3rpx;
}

.storeRigth {
  width: 16%;
  text-align: right;
}

.callBtn {
  width: 110rpx;
  display: inline-block;
  /* height: 45rpx; */
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #fff;
  border-radius: 8rpx;
  text-align: center;
}

.distance {
  font-size: 24rpx;
  color: #333;
  margin-top: 90rpx;
  text-align: center;
}

/*栏目*/

.storeColum {
  background: #fff;
  border-radius: 8rpx;
  border: 1rpx solid #dedede;
  box-shadow: #dedede 0px 0px 8px;
}

.storeColum {
  padding: 0 0 35rpx 0;
  margin-top: 15rpx;
}

.columTitle {
  padding: 20rpx 30rpx;
  color: #666;
}

.itemLabel {
  padding: 0 20rpx;
  display: inline-block;
  border-left: #45cdc6 solid 5rpx;
  font-size: 30rpx;
}

.view-mall {
  margin: 0 20rpx;
  position: relative;
}

/*我的预约*/

.mydistOne {
  padding: 25rpx;
  background: #fff;
  border-radius: 8rpx;
  margin-top: 10rpx;
  display: flex;
  flex-flow: row;
  box-shadow: 0px 5rpx 15rpx #dedede;
}

.mydistLeft {
  width: 126rpx;
}

.mydistLeft image {
  width: 126rpx;
  height: 126rpx;
  border-radius: 16rpx;
  box-shadow: #999 0 0 10rpx;
}

.mydistCenter {
  width: 55%;
  padding: 0 20rpx;
}

.mydistTitle {
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  font-weight: bold;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.mydistDate {
  font-size: 24rpx;
  color: #999;
  margin-top: 55rpx;
}

.mydistRigth {
  width: 20%;
  text-align: right;
}

.mydistip {
  color: #c33;
  font-size: 24rpx;
}

.mydistBtn {
  width: 125rpx;
  display: inline-block;
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #fff;
  background: #45cdc6;
  border-radius: 8rpx;
  text-align: center;
  margin-top: 55rpx;
}

/*发现的列表样式*/

.disone {
  margin-bottom: 10rpx;
  background-color: #fff;
}

.info {
  display: flex;
  flex-direction: row;
  padding: 20rpx 30rpx;
  height: 180rpx;
}

.infoletf {
  width: 60%;
  position: relative;
}

.inforight {
  width: 30%;
  text-align: right;
}

.inforight image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
}

.title {
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.disbottom {
  font-size: 24rpx;
  color: #999;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  flex-flow: row;
}

.disread {
  margin-right: 30rpx;
}

.ydImg {
  width: 35rpx;
}

.hfImg {
  width: 29rpx;
}

.dzImg {
  width: 26rpx;
}

.selectOne {
  width: 10%;
}

/*服务卡*/

.service-list, .service-list2, service-list3 {
  background-color: #fff;
  position: relative;
  overflow: hidden;
  display: inline-block;
}

.service-list {
  width:225rpx;
  height: 283rpx;
  margin: 0rpx 0 0 10rpx;
}

.service-list2 {
  width: 340rpx;
  height: 340rpx;
  margin: 0 0 0 10rpx;
}

.service-list3 {
  width: 24.7%;
  height: 290rpx;
}

.service-list3:not(:first-child) {
  border-left: 1rpx solid #f5f5f5;
}

.service-list .mall-list-img {
  position: relative;
  /* padding: 10rpx 10rpx 0 10rpx;  */
  overflow: hidden;
  height:156rpx;
}

.service-list .mall-list-img image {
  width: 100%;
 height:156rpx;
  opacity: 1;
  border-radius:10rpx
}

.shopbottom {
  padding: 0;
}

.service-list2 .mall-list-img {
  position: relative;
  /* padding: 10rpx; */
  overflow: hidden;
  height: 216rpx;
}

.service-list2 .mall-list-img image {
  width: 100%;
  height: 216rpx;
  border-radius:10rpx
}

.service-list3 .mall-list-img {
  position: relative;
  padding: 10rpx 10rpx 0 10rpx;
  overflow: hidden;
  height: 170rpx;
}

.service-list3 .mall-list-img image {
  width: 100%;
  height: 170rpx;
  opacity: 1;
}

.mall_title {
  padding: 0 10rpx 0 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.mall_title .mall-listword {
  color: #333;
  font-size: 28rpx;
  margin-top: 10rpx;
  text-align: left;
}

.privefont {
  color: #e4393c;
}

.kucun {
  color: #8f8f94;
}

.ku {
  font-size: 20rpx;
}

.shop-prive {
  margin-top: 10rpx;
  font-size: 24rpx;
  clear: both;
  margin-left: 5rpx;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.mall-listprice {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 22rpx;
  text-align: center;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  color: #c33;
  padding: 10rpx 0rpx 0rpx 10rpx;
  height: 30rpx;
}

.red {
  color: #f02804;
  padding: 0 12rpx 0 16rpx;
}

@media screen and (max-width:360px) and (max-height:640px) {
  .service-list {
    width: 226rpx;
  }
  .service-list2 {
  margin: 0 0 0 12rpx;
  } 
}
@media screen and (min-width:414px) and (max-height:736px) {
  .service-list2 {
  margin: 0 0 0 12rpx;
}
}