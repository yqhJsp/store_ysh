/* pages/marketing/distribution/distribution.wxss */
.fr{
    float: right      
}
.clear{
  clear:both      
}
page{
  background: #fff
}
.row{
   box-sizing: border-box;
   font-size: 26rpx;
   margin-bottom:20rpx;
   width:80%;
   margin: 0 auto    
}
.coverImg image{
  width: 100%;
  height:418rpx
}
.consulTitle{
  margin:120rpx 0 30rpx 0;
  padding:0 30rpx     
}
.consulTitle .clable{
   font-size:32rpx;
   color:#000     
}
.icon-popups{
 position:relative;
 font-size:24rpx;
 color: #999
}
.payname {
    margin-left: 40rpx;
}
.order {
    padding: 0 30rpx;
}
.order-detail {
    margin-top:0;
}
.order_table{
  padding:0 30rpx;      
  border-right:0;
  background-color: #fff
}
.orderth{
  font-size:28rpx;
   color:#333;     
}
.ordertr{
 padding:20rpx 0 10rpx 0;
  width: 100%;
   display: flex; 
  justify-content: space-between      
}
.ordertd {
  height:100%;
   width: 100%;
   text-align:left;
}
.ortitle{
  width:25%;   
}
.oricon{
    width:10%    
}
.orderTip{
  width:30%;
  margin-left:20rpx       
}
.ordertd view{
  margin-bottom:20rpx;      
}
.borders{
  border-bottom:1rpx solid #dedede;       
}
.fail_img{
  height:140rpx;
  width: 140rpx;      
}
.tip{
  margin-top:30rpx
}
/*商品列表*/
.order_one{
    width: 100%;
   display: flex;
   justify-content: space-between          
}
.order_one navigator{
    height:160rpx;
    width:98%;
    position: relative;
     margin-top:20rpx;   
    border-bottom: 1px solid #DEDEDE;     
}
.product-info-box text{  
  display: block;   
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
} 
.shoptitle{
  font-size:26rpx;       
}
 .pro-img {
    position: absolute;
    height:120rpx;
    width: 120rpx;
    overflow: hidden;
    padding: 10rpx;
    text-align: center;
    border: 1px solid #EDEDED;
}
 .pro-img image{
   height:120rpx;
    width: 120rpx;     
}
.product-info-box {
    -webkit-box-flex: 1;
    margin-left:160rpx;
    height: 100%;
    margin-top: -2px;
    padding-right: 10rpx;
}
.futile {
    margin: 6rpx 0;
    font-size: 24rpx;
    color: #999;
    line-height: 1;
}
.nav_prive{
 position:absolute;
 bottom:20rpx;
 width:70%;
 left:160rpx;
 font-size:24rpx       
}
.red-price{
 color: #f23030;
}
.little-icon {
  color: #9B9B9B;
}
.addressbun{
  width: 100%;
 text-align: center;   
}
.goback {
    background: #F02804;
    position:relative;
    color: #fff;
    display:inline-block;
    font-size: 26rpx;
    text-align: center;
    margin-top:70rpx;
    margin-bottom:30rpx;
    margin-right:20rpx;
    width:200rpx;
    height:60rpx;
    padding-top: 0.6rem;
    border-radius: 12px;
}
.refut {
    color: #999;
}
.addinput {
    vertical-align: top;
}
.refund-desc {
    width:96%;
    height: 200rpx;
    margin: 0 auto;
    border-radius:12rpx;
    margin-top:20rpx;
    border: 1rpx #D9D9D9 solid;
    text-align: left;
    padding:10rpx
}
.fail-item {
    border-top: 1rpx #dedede solid;
     width: 100%;
}
.refundContent {
    height: 150rpx;
    margin-top:20rpx;
    margin-left:30rpx
}
.refundContent .img_result {
    margin: 20rpx 0;
}
.refundContent .img_result image{
   width:100rpx;
   height:100rpx;
   margin-right:10rpx;
   margin-top:10rpx    
}
.saveBtn {
  display: block;
    border: 1rpx solid #dedede;
   height:50rpx;
   width: 150rpx;
   padding-top:15rpx; 
   text-align: center;
    font-size:26rpx;
    border-radius: 12rpx;
}
.mod_btn {
    background:#f44336;
    color: #fff;
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    border: 0;
    margin: 0 auto;
    width: 96%;
    height: 92rpx;
    margin-top: 50rpx;
    line-height: 92rpx;
    text-align: center;
    font-size: 32rpx;
    border-radius:50rpx;
    position: relative;
}
.gofen{
  width:510rpx;
  position:absolute;
  bottom:60rpx;
  left:20%;
  background:#f44336;
  color: #fff;
  font-size:28rpx;
  height:93rpx;
  text-align: center;
  border-radius:50rpx;
  line-height:93rpx
}
.session{
  width:100%;
  text-align:center
}
.distip{
  width: 342rpx;
  height: 311rpx;
  margin-top: 200rpx;
}