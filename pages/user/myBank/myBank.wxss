/*弹窗样式*/
.commodity_screen {
  background-color: rgba(0, 0, 0, 0.7);
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: fixed;
  z-index: 666;
  bottom: 0;
}

.coupon_area {
  min-height: 600rpx;
  max-height: 800rpx;
  position: fixed;
  z-index: 888;
  bottom: 0;
  width: 100%;
  overflow-y: auto;
}
.coupon_area {
  background-color: #fff;
}

.display-block {
  display: block !important;
}

.display-none {
  display: none !important;
}

.couponTitle {
  position:relative;
  top: 0;
  width: 100%;
  padding: 20rpx 0;
}

.coupon_tip {
  font-size: 30rpx;
  text-align: center;
}

.coupon_no {
  text-align: center;
}

.couponClose {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.couponClose {
  padding: 20rpx;
}

.coupon_no {
  text-align: center;
}

.coupon_no image {
  width: 165rpx;
  height: 179rpx;
  margin-top: 60rpx;
}

.couponDome {
  height: 600rpx;
  margin-top:30rpx;
  text-align: center;
}
