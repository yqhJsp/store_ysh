}
/*商品列表*/
.order_one{
    width: 100%;
   display: flex;
   justify-content: space-between          
}
.order_one .navigator{
    height:160rpx;
    width:98%;
    position: relative;
     margin-top:20rpx;   
    border-bottom: 1px solid #DEDEDE;     
}
.product-info-box text{  
  display: block;   
  /*设置文字溢出部分为...*/  
  overflow: hidden;  
  white-space: nowrap;  
  text-overflow: ellipsis;  
} 
.shoptitle{
  font-size:28rpx;       
}
 .pro-img {
    position: absolute;
    height:120rpx;
    width: 120rpx;
    overflow: hidden;
    padding: 10rpx;
    text-align: center;
    border: 1px solid #EDEDED;
}
 .pro-img image{
   height:120rpx;
    width: 120rpx;     
}
.product-info-box {
    -webkit-box-flex: 1;
    margin-left:160rpx;
    height: 100%;
    margin-top: -2px;
    padding-right: 10rpx;
}
.futile {
    margin: 6rpx 0;
    font-size: 24rpx;
