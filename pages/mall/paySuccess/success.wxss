/* pages/mall/paySuccess/success.wxss */
page{
  background-color: #f5f5f5      
}
.row{
   box-sizing: border-box;
   font-size: 28rpx;     
}
.success-tip {
    padding: 12rpx 30rpx;
    color: #333;
    font-size: 28rpx;
    text-align: center;
    margin-bottom:50rpx;
}
.success-tab {
    margin-top: 50rpx;
}
.success-btn {
    display: inline-block;
    padding: 20rpx 50rpx;
    margin-right: 0.5rem;
    color: #fff;
    border-radius: 12rpx;
    background-color: rgba(240,40,4,0.9);
}
.list-title {
    font-size: 28rpx;
    margin: 0 20rpx;
    padding: 0 20rpx;
    border-left: 4rpx solid #FF0000;
}
.success-list {
    margin-top:10rpx;
     width: 100%;
}
.mall-contact {
    margin: 10px 0;
}
.mall-one {
    width:360rpx;
    display:inline-block;
    height:474rpx;
    overflow:hidden;
    box-shadow: #929292;
    background-color: #fff;
    margin-left:10rpx;
    margin-bottom: 10rpx; 
}
.mall-one .shopImg {
    position: relative;
    height:340rpx;
    text-align: center;
    padding:10rpx
}
 .mall-one .shopImg image{
     width:100%;
     height:340rpx;  
}
.shopbottom{
  padding:0 10rpx
}
 .mall-one .mall-listword {        
display:block;
font-size:28rpx;
text-align:left;
overflow:hidden;
white-space:nowrap;
margin:10rpx 0;
text-overflow:ellipsis;
}
.shop-prive {
  font-size:28rpx;
  clear:both
}
.fl{
float:left
}
.fr{
float:right
}
.privefont {
    color: #E4393C;
}
.kucun {
    color: #8f8f94;
}
